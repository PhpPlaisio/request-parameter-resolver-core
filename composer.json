{
  "name": "plaisio/request-parameter-resolver-core",
  "description": "PhpPlaisio: Core Request Parameter Resolver",
  "keywords": [
    "PhpPlaisio",
    "Core",
    "Request Parameter Resolver"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.3",
    "plaisio/request-parameter-resolver": "^2.0.0"
  },
  "require-dev": {
    "phing/phing": "^3.0.1",
    "phpunit/phpunit": "^11.5.25",
    "setbased/error-handler": "^1.4.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\RequestParameterResolver\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\RequestParameterResolver\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
