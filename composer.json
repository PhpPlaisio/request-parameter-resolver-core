{
  "name": "plaisio/request-parameter-resolver-core",
  "description": "PhpPlaisio: Core Request Parameter Resolver",
  "keywords": [
    "PhpPlaisio",
    "Core",
    "Request Parameter Resolver"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.4",
    "plaisio/kernel": "^3.1.1",
    "plaisio/request": "^0.11",
    "plaisio/request-parameter-resolver": "^1.2"
  },
  "require-dev": {
    "phing/phing": "^2.16.3",
    "phpunit/phpunit": "^9.5",
    "plaisio/console": "^2.2.6",
    "plaisio/request-core": "^0.11"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\RequestParameterResolver\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\RequestParameterResolver\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
